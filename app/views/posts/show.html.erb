<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>
<div class="image">
  <%= image_tag @post.image.url if @post.image? %>
</div>
<div class="video">
  <%= youtube_video @post.video if @post.video? %>
</div>
<p>
  <strong>Content:</strong>
  <%= @post.content %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
